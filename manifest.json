{
  "manifest_version": 2,

  "name": "Talloc",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],

      "js": ["script.js"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
    }
}
